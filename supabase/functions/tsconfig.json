{
  "compilerOptions": {
    "allowJs": true,
    "lib": ["ES2022", "DOM"],
    "module": "ES2022",
    "target": "ES2022",
    "strict": false,
    "noEmit": true,
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "allowImportingTsExtensions": true,
    "types": []
  },
  "include": [
    "**/*.ts",
    "deno.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
